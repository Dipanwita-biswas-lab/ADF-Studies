{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "az-adf-dipa-ADF"
		},
		"AmazonRedshift1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AmazonRedshift1'"
		},
		"AmazonRedshift1_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "redshift.prd.rwd.norstella-labs.com"
		},
		"AmazonRedshift1_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "rwd_production"
		},
		"AmazonRedshift1_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "biswas_dipanwita"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "az-adf-dipa-ADF_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AmazonRedshift1_password": {
						"value": "[parameters('AmazonRedshift1_password')]"
					},
					"AmazonRedshift1_properties_typeProperties_server": {
						"value": "[parameters('AmazonRedshift1_properties_typeProperties_server')]"
					},
					"AmazonRedshift1_properties_typeProperties_database": {
						"value": "[parameters('AmazonRedshift1_properties_typeProperties_database')]"
					},
					"AmazonRedshift1_properties_typeProperties_username": {
						"value": "[parameters('AmazonRedshift1_properties_typeProperties_username')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}